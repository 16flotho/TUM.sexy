#Turn on the magic
RewriteEngine On

#Ignore direct access to the redirect folder
RewriteRule ^redirect - [L,NC]

RewriteEngine on
RewriteCond %{HTTP_HOST} !www.tum.sexy [NC]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.*)$ /redirect/index.php [L]

